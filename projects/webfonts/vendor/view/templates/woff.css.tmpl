@charset 'utf-8';
{{range .}}@font-face {
	font-family: '{{.Family}}';
	{{- if .Base64Data}}
	src: url(data:{{.MimeType}};charset=utf-8;base64,{{.Base64Data}}) format('{{.Format}}');
	{{else}}
	src: url('{{.Url}}') format('{{.Format}}');
	{{- end}}
	{{- if .Display}}
	font-display: swap;
	{{- end}}
	font-style: {{.Style}};
	font-weight: {{.Weight}};
	{{- if .Range}}
	unicode-range: {{.Range}};
	{{- end}}
}
{{end}}
